{
  "name": "Contacts-api",
  "version": "1.0.0",
  "description": "Contacts api with Typescript, MySQL, NodeJs, Express db-migrate",
  "main": "src/server.ts",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "create:db": "db-migrate db:create contacts",
    "prettier": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "dev": "tsc-watch --noClear -p tsconfig.json --onSuccess \"node ./build/server.js\"",
    "test": "set ENV=test && db-migrate db:create contacts_test && db-migrate --env test up && jasmine && db-migrate db:drop contacts_test"
  },
  "keywords": [
    "Typescript",
    "MYSQL",
    "NodeJs",
    "Express",
    "db-migrate"
  ],
  "author": "Godswill Edet",
  "license": "GPL-3.0",
  "dependencies": {
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-mysql": "^2.2.0",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "pg": "^8.7.3",
    "supertest": "^6.2.4",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.0.1",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-config-google": "^0.14.0",
    "jasmine": "^4.2.1",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "prettier": "^2.7.1",
    "tsc-watch": "^5.0.3"
  }
}
